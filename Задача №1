void main() {
  

 List<int> list = [1, 2, 3, 4, 5, 6, 7, 1 , 8, 9, 10, 11];
  


 print("Задача №1");
 print(findFirstUniqueNumber(list));
 
 print("Задача №2");
 print(findIndexesGivesSum(list,10));
  
  
}
int findFirstUniqueNumber(List<int> array) {
 
  
 for (var element in array){
   
   final indexElement = array.indexOf(element,0);
   
   final nextElement = indexElement + 1;
   

   if (array.indexOf(element,nextElement) == -1) {
     
     return element; 
    
   };
  
 };
  
  
  return -1;
}


List<int> findIndexesGivesSum(List<int> array, int summaEtalon) {

  final count = array.length; 
  
  var firstIndex = 0;
  var lastIndex = count-1;
  
  while (firstIndex != lastIndex)
    {       
        int summa = array[firstIndex] + array[lastIndex];
    
    
        if (summa < summaEtalon)
            firstIndex++;
        else if (summa > summaEtalon)
            lastIndex--;
        else 
        {
            return <int> [firstIndex,lastIndex];
        }
    }
  
  return <int> [-1,-1];
}
